package {	import flash.utils.Timer;	import flash.display.MovieClip;	import com.greensock.TweenNano;	import fl.motion.easing.Cubic;public class Text extends MovieClip{	public var timer:Timer = new Timer(3000);	public var timer2:Timer = new Timer(3000, 1);		public function Text()	{ 		TweenNano.to(this,1.5,{alpha:1, ease:Cubic.easeIn});		timer.start();				timer2.start();				TweenNano.to(this.aral,1.5,{alpha:1,delay:1, ease:Cubic.easeIn});			}	}}