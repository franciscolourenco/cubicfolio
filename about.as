package  {		import flash.events.MouseEvent;	import flash.events.Event;	import flash.net.navigateToURL;	import flash.net.URLRequest;	import flash.display.MovieClip;			public class about extends MovieClip{		public function about() {						this.facebookB.addEventListener(MouseEvent.MOUSE_UP, function(e:Event){				navigateToURL(new URLRequest("http://www.facebook.com/aristidesfl"));			});						this.skypeB.addEventListener(MouseEvent.MOUSE_UP, function(e:Event){				navigateToURL(new URLRequest("skype:aristideskype?chat"));			});						this.twitterB.addEventListener(MouseEvent.MOUSE_UP, function(e:Event){				navigateToURL(new URLRequest("http://twitter.com/aristidesfl"));			});						this.mailB.addEventListener(MouseEvent.MOUSE_UP, function(e:Event){				navigateToURL(new URLRequest("mailto:aristidesfl@gmail.com"));			});						this.facebookB.addEventListener(MouseEvent.MOUSE_OVER, over);			this.skypeB.addEventListener(MouseEvent.MOUSE_OVER, over);			this.twitterB.addEventListener(MouseEvent.MOUSE_OVER, over);			this.mailB.addEventListener(MouseEvent.MOUSE_OVER, over);						this.facebookB.addEventListener(MouseEvent.MOUSE_OUT, out);			this.skypeB.addEventListener(MouseEvent.MOUSE_OUT, out);			this.twitterB.addEventListener(MouseEvent.MOUSE_OUT, out);			this.mailB.addEventListener(MouseEvent.MOUSE_OUT, out);								}				function over(e)		{			e.currentTarget.alpha = 1;		}				function out(e)		{			e.currentTarget.alpha = 0.6;		}	}   }